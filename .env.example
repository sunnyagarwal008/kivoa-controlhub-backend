# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/product_db

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-bucket-name

# AWS SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/your-queue-name

# Flask Configuration
FLASK_APP=src.app
FLASK_ENV=development
SECRET_KEY=your-secret-key-here

# S3 Presigned URL Expiration (in seconds)
PRESIGNED_URL_EXPIRATION=3600

# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-1.5-flash

# Image Enhancement Configuration
ENHANCED_IMAGES_COUNT=3

# Shopify Configuration
SHOPIFY_STORE_URL=https://your-store.myshopify.com
SHOPIFY_ACCESS_TOKEN=your-shopify-access-token
SHOPIFY_API_VERSION=2024-07

